{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "X-Frame-Options",
            "value": "SAMEORIGIN"
          },
          {
            "key": "Content-Security-Policy",
            "value": "frame-src youtube.com https://www.youtube.com"
          }
        ]
      }
    ]
  }